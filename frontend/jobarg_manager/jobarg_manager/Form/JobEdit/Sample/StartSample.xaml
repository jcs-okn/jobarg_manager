    <UserControl x:Class="jp.co.ftf.jobcontroller.JobController.Form.JobEdit.StartSample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             FontSize="11" FontFamily="Meiryo">
    <Canvas x:Name="LayoutRoot"    Background="Transparent" >

        <Rectangle Canvas.ZIndex="100"  Name="picStart" Height="23" Width="70"
                   RadiusX="10" RadiusY="100" Stroke="Black" Fill="White"
                   Canvas.Left="0" Canvas.Top="0">

        </Rectangle>
        <TextBlock   Canvas.ZIndex="1000" Name="tbJobName" Foreground="Black" Text="START"    
                     VerticalAlignment="Center" HorizontalAlignment="Center" TextWrapping="Wrap"  MaxWidth="90"
                     Height="15" Width="70"  Canvas.Left="0" Canvas.Top="5" FontWeight="Normal" TextAlignment="Center">

        </TextBlock>
    </Canvas>


</UserControl>
