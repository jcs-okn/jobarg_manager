    <UserControl x:Class="jp.co.ftf.jobcontroller.JobController.Form.JobEdit.MtsSample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             FontSize="11" FontFamily="Meiryo">
    <Canvas x:Name="LayoutRoot"    Background="Transparent" >

        <Rectangle Canvas.ZIndex="100"  Name="picParStart" Height="25" Width="25" Stroke="Black"
                   Canvas.Top="0" Canvas.Left="0" Fill="White" >
        </Rectangle>

        <Polyline Canvas.ZIndex="101" Name="charParEnd1" Points="6,18 6,7 13,7 13,18"
                  Canvas.Top="0" Canvas.Left="0"
                  Stroke="Black"  StrokeThickness="2">
        </Polyline>
        <Polyline Canvas.ZIndex="101" Name="charParEnd2" Points="13,7 20,7 20,18"
                  Canvas.Top="0" Canvas.Left="0"
                  Stroke="Black"  StrokeThickness="2">
        </Polyline>
    </Canvas>


</UserControl>
